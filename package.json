{
  "name": "@validify-js/core",
  "version": "1.0.0",
  "description": "validation package for js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "",
    "build": "rm -rf dist/ && tsc",
    "git": "git add . && git commit -m \"$npm_package_name ($npm_package_version)\"",
    "pub": "npm run git && npm version patch && git push origin master && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmansimli/validify-js-core.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "nodejs",
    "validation"
  ],
